{
    "name": "neocom.infinity",
    "version": "0.19.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "npm run build:prod",
        "test": "ng test",
        "lint": "ng lint",
        "compodoc": "compodoc -p tsconfig.json --theme laravel",
        "typedoc": "typedoc --mode modules -module commonjs --exclude **/*.spec.ts --out docs/typedoc src/app",
        "start:support": "bash ./support/support-services.sh start",
        "start:dev": "ng serve --progress --proxy-config proxy.conf.json --base-href /app --public-host neocom.infinity.local",
        "start:server": "npm run build:dev && node server.js",
        "stop:support": "bash ./support/support-services.sh stop",
        "build:dev": "ng build --base-href /app",
        "build:prod": "ng build --aot --prod --configuration production --base-href /app",
        "cucumber": "./node_modules/cucumber/bin/cucumber-js e2e/src/features/",
        "test:dev": "ng test --code-coverage --source-map --watch --progress ",
        "test:prod": "ng test --watch=false --no-progress --browsers=ChromeHeadlessCI",
        "e2e": "ng e2e --port 4300 --configuration=e2e",
        "e2e:open": "cypress open",
        "e2e:atdd": "ng e2e --port 4300 --host neocom.infinity.local --protractor-config e2e/protractor-cucumber.conf.js",
        "e2e:compile": "tsc -p e2e"
    },
    "engines": {
        "node": "10.16.3",
        "npm": "6.11.3"
    },
    "cypress-cucumber-preprocessor": {
        "nonGlobalStepDefinitions": true,
        "cucumberJson": {
            "generate": true,
            "outputFolder": "cypress/cucumber-json",
            "filePrefix": "",
            "fileSuffix": ".cucumber"
        }
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "8.2.14",
        "@angular/common": "8.2.14",
        "@angular/compiler": "8.2.14",
        "@angular/core": "8.2.14",
        "@angular/forms": "8.2.14",
        "@angular/platform-browser": "8.2.14",
        "@angular/platform-browser-dynamic": "8.2.14",
        "@angular/router": "8.2.14",
        "angular-webstorage-service": "1.0.2",
        "bootstrap": "4.3.1",
        "compression": "1.7.4",
        "config": "3.2.3",
        "date-fns": "2.5.0",
        "express": "4.17.1",
        "express-http-proxy": "1.6.0",
        "font-awesome": "4.7.0",
        "jwt-decode": "2.2.0",
        "ng-drag-drop": "5.0.0",
        "ng6-toastr-notifications": "1.0.4",
        "ngx-toastr": "10.2.0",
        "path": "0.12.7",
        "request": "2.88.0",
        "rollbar": "2.13.0",
        "rxjs": "6.4.0",
        "tslib": "1.10.0",
        "zone.js": "0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.803.14",
        "@angular/cli": "8.3.14",
        "@angular/compiler-cli": "8.2.14",
        "@angular/language-service": "8.2.11",
        "@bahmutov/add-typescript-to-cypress": "2.1.2",
        "@cypress/webpack-preprocessor": "4.1.1",
        "@types/chai": "4.2.2",
        "@types/cucumber": "4.0.7",
        "@types/jasmine": "3.3.8",
        "@types/jasminewd2": "2.0.3",
        "@types/node": "8.9.4",
        "chai": "4.2.0",
        "chromedriver": "79.0.0",
        "codelyzer": "5.0.0",
        "cucumber": "5.1.0",
        "cypress": "3.8.2",
        "cypress-cucumber-preprocessor": "^1.19.2",
        "jasmine-core": "3.4.0",
        "jasmine-spec-reporter": "4.2.1",
        "karma": "4.1.0",
        "karma-chrome-launcher": "2.2.0",
        "karma-coverage-istanbul-reporter": "2.0.1",
        "karma-jasmine": "2.0.1",
        "karma-jasmine-html-reporter": "1.4.0",
        "protractor": "5.4.2",
        "protractor-console-plugin": "0.1.1",
        "protractor-cucumber-framework": "6.1.3",
        "protractor-multiple-cucumber-html-reporter-plugin": "1.8.1",
        "selenium-webdriver": "^4.0.0-alpha.5",
        "ts-loader": "6.2.1",
        "ts-node": "7.0.0",
        "tslint": "5.15.0",
        "typescript": "3.5.3",
        "webpack": "4.41.5"
    }
}
