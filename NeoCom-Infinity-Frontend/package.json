{
  "name": "neocom-infinity",
  "version": "0.20.1-mining-sessions.1",
  "scripts": {
    "cucumber": "$(npm bin)/cucumber-js ./cypress/**/*.feature --no-strict --tags @NIF07.01",
    "docker:start": "docker-compose -f ./support/neocom-docker-frontend/docker-compose.yml up & ",
    "docker:stop": "docker-compose -f ./support/neocom-docker-frontend/docker-compose.yml down ",
    "serve:dev": "ng serve --progress --proxy-config proxy.conf.json --port 5200 --base-href /app ",
    "build:dev": "ng build ",
    "document:dev": "compodoc -p tsconfig.json src",
    "build:stage": "ng build --aot --configuration=stage",
    "build:prod": "ng build --aot --configuration=production",
    "test:dev": "ng test --code-coverage --watch --progress ",
    "test:prod": "ng test --watch=false --no-progress --browsers=ChromeHeadlessCI",
    "cypress:run": "cypress run ",
    "acceptance:ready": "export NODE_ENV=integration && export PORT=5200 && npm start &",
    "acceptance:cypress:NIF01": "cypress run --spec ./cypress/acceptance/NIF01-LoginAuthentication.feature",
    "acceptance:cypress:NIF02": "cypress run --spec ./cypress/acceptance/NIF02-DashboardHome.feature",
    "acceptance:cypress:NIF07": "cypress run --spec ./cypress/acceptance/NIF07-BlueprintCostIndex.feature",
    "acceptance:cypress:NIF09": "cypress run --spec ./cypress/acceptance/NIF09-IndustryDashboard.feature",
    "heroku-postbuild": "ng build --configuration=${NODE_ENV}",
    "serve:integration": "export NODE_ENV=integration && export PORT=5202 && npm run build:dev && npm start"
  },
  "private": true,
  "engines": {
    "node": "v18.19.1",
    "npm": "10.2.4"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "resolutions": {
    "webpack": "^5.0.0"
  },
  "dependencies": {
    "@angular/animations": "12.2.17",
    "@angular/cdk": "12.2.13",
    "@angular/common": "12.2.17",
    "@angular/compiler": "12.2.17",
    "@angular/core": "12.2.17",
    "@angular/forms": "12.2.17",
    "@angular/material": "12.2.13",
    "@angular/platform-browser": "12.2.17",
    "@angular/platform-browser-dynamic": "12.2.17",
    "@angular/router": "12.2.17",
    "@bahmutov/add-typescript-to-cypress": "2.1.2",
    "@swimlane/ngx-charts": "16.0.0",
    "@types/uuid": "7.0.3",
    "child-process": "1.0.2",
    "compression": "1.7.4",
    "config": "3.3.1",
    "date-fns": "^2.16.1",
    "express": "4.17.1",
    "express-http-proxy": "1.6.0",
    "fs": "0.0.1-security",
    "jwt-decode": "2.2.0",
    "ng-drag-drop": "5.0.0",
    "ngx-toastr": "12.0.1",
    "ngx-webstorage-service": "4.1.0",
    "npm": "10.2.4",
    "path": "0.12.7",
    "request": "2.88.2",
    "rxjs": "6.5.4",
    "sass": "^1.71.1",
    "sass-loader": "10.0.1",
    "tslib": "1.10.0",
    "uuid": "8.0.0",
    "wait-on": "5.0.0",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.18",
    "@angular/cli": "12.2.18",
    "@angular/compiler-cli": "12.2.17",
    "@cypress/code-coverage": "3.8.1",
    "@cypress/webpack-preprocessor": "5.2.1",
    "@types/chai": "4.2.11",
    "@types/cucumber": "6.0.1",
    "@types/cypress-cucumber-preprocessor": "1.14.1",
    "@types/jasmine": "3.5.0",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "12.11.1",
    "chai": "4.2.0",
    "chromedriver": "81.0.0",
    "codelyzer": "6.0.0",
    "cucumber": "6.0.5",
    "cypress": "^6.6.0",
    "cypress-cucumber-preprocessor": "2.3.1",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "6.4.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "3.0.1",
